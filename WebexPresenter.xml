      <Row>
        <Name>To save presets: </Name>
        <Widget>
          <WidgetId>wbxpresent_text_save_presets</WidgetId>
          <Name>After making changes, hold the Preset button on the Webex Presenter page for 2 seconds. </Name>
          <Type>Text</Type>
          <Options>size=4;fontSize=normal;align=center</Options>
        </Widget>
      </Row>
      <PageId>wbxpresent_panel_move_preset</PageId>
      <Options>hideRowNames=0</Options>
    </Page>
    <Page>
      <Name>Demos</Name>
      <Row>
        <Name>3 video sources demo</Name>
        <Widget>
          <WidgetId>wbxpresent_video_composite</WidgetId>
          <Name>Camera &amp; USB-C &amp; HDMI</Name>
          <Type>Button</Type>
          <Options>size=4</Options>
        </Widget>
