        <Name>Opacity (Blend only)</Name>
        <Widget>
          <WidgetId>widget_opacity_text</WidgetId>
          <Name>?</Name>
          <Type>Text</Type>
          <Options>size=1;fontSize=normal;align=center</Options>
        </Widget>
        <Widget>
          <WidgetId>wbxpresent_slider_opacity</WidgetId>
          <Type>Slider</Type>
          <Options>size=3</Options>
