      <Row>
        <Name>Move overlay</Name>
        <Widget>
          <WidgetId>wbxpresent_move</WidgetId>
          <Name>OK</Name>
          <Type>DirectionalPad</Type>
          <Options>size=4</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Scale</Name>
        <Widget>
          <WidgetId>widget_3</WidgetId>
          <Type>Spacer</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>wbxpresent_zoom_in</WidgetId>
          <Type>Button</Type>
